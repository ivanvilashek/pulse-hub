generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model User {
  id        String  @id @default(dbgenerated("gen_random_uuid()"))
  password  String
  email     String  @unique
  firstName String
  lastName  String
  image     String?
}
